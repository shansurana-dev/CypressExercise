{
    "include": [
        "node_modules/cypress",
        "cypress/**/*.js",
        "src/**/*",
        "**/*.ts"
    ],
    "compilerOptions": {
        "baseUrl": "../node_modules",
        "paths": {
            "~/*": [
                "../src/*"
            ]
        },
        "jsx": "react",
        "target": "es6",
        "lib": [
            "es6",
            "dom",
            "es2017.string"
        ],
        "types": [
            "cypress"
        ],
        "moduleResolution": "node"
    }
}